impl<A: Array + AsRef<[<A as Array>::Item]> + AsMut<[<A as Array>::Item]>> Drop for ArrayQueue<A> {
    fn drop(&mut self) {
        for x in self {
            drop(replace(x, unsafe { uninitialized() }));
        }
    }
}'''

impl Drop for MmapMemory {
	fn drop(&mut self) {
		if self.memory_size() > 0 {
			unsafe {
				if munmap(self.host_address() as *mut c_void, self.memory_size()).is_err() {
					panic!("munmap failed");
				}
			}
		}
	}

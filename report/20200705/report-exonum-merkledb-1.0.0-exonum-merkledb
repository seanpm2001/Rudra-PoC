source = '''
impl<T> Drop for NoDropOnShutdown<T> {
    #[allow(unsafe_code)]
    fn drop(&mut self) {
        if !FINISHED.load(Ordering::Acquire) {
            unsafe {
                // SAFETY: `self.inner` is not used afterwards.
                mem::ManuallyDrop::drop(&mut self.inner);
            }
        }
    }
}'''

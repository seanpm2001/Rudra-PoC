impl Drop for Terminal {
    fn drop(&mut self) {
        if let Err(e) = self.set_cursor_mode(CursorMode::Normal) {
            eprintln!("failed to restore terminal: {}", e);
        }

        if self.owned_fd {
            unsafe { close_fd(self.out_fd); }
        }
    }
}'''

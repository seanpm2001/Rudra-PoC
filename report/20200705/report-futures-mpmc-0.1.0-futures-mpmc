impl<T> Drop for Channel<T> {
    fn drop(&mut self) {
        // Drain the buffer.
        while let Some(Some(_)) = self.try_receive() {}
        unsafe {
            let _ = Vec::from_raw_parts(self.buf, 0, self.capacity());
        }
    }
}'''

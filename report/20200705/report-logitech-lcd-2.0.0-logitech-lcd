impl Drop for Lcd {
    /// Kills the applet and frees memory used by the SDK
    fn drop(&mut self) {
        unsafe {
            (self.lib.LogiLcdShutdown)();
        }
        INITIALIZED.store(false, Ordering::SeqCst);
    }
}'''

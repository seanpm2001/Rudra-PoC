impl Drop for MemoryMap {
    /// Unmap the mapping. Panics if `munmap` panics.
    fn drop(&mut self) {
        if self.len == 0 {
            return;
        }

        unsafe {
            // `munmap` only panics due to logic errors.
            libc::munmap(self.data as *mut c_void, self.len as libc::size_t);
        }
    }
}'''

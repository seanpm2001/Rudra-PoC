impl Drop for SharedObject {
    fn drop(&mut self) {
        if self.handle.is_null() {
            unsafe {
                #[cfg(windows)]
                FreeLibrary(self.handle);
                #[cfg(unix)]
                dlclose(self.handle);
            }
            self.handle = std::ptr::null_mut();
        }
    }

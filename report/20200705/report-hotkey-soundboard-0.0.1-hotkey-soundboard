impl Drop for Listener {
    fn drop(&mut self) {
        if let Err(err) = self.sender.send(HotkeyMessage::DropThread) {
            eprintln!("cant send close thread message {}", err);
        }
        unsafe {
            (self.xlib.XCloseDisplay)(self.display);
        }
    }
}'''

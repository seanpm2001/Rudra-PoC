impl<T> Drop for Bucket2<T> {
    fn drop(&mut self) {
        for item in self.slot.iter_mut() {
            if item.is_null() {
                continue;
            }

            unsafe { ptr::drop_in_place(*item); }
            *item = ptr::null_mut();
        }
    }
}'''

impl<T: 'static + Send + Sync> Drop for Stm<T> {
    fn drop(&mut self) {
        let guard = crossbeam_epoch::pin();
        let shared = self.inner.load(Ordering::Acquire, &guard);
        unsafe {
            shared.into_owned();
        }
    }
}'''

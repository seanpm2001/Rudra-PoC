[[reports]]
level = 'Warning'
analyzer = 'UnsafeDestructor'
description = 'unsafe block detected in drop'
location = 'src/pty/mod.rs:134:1: 141:2'
source = '''
impl Drop for PTY {
    fn drop(&mut self) {
        assert!(
            unsafe { libc::close(self.as_raw_fd()) } == 0,
            format!("Closing PTY failed ({}).", last_error())
        );
    }
}'''

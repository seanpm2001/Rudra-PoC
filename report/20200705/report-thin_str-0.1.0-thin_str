impl Drop for ThinStr {
    #[inline]
    fn drop(&mut self) {
        let len = self.len();
        if len > 0 {
            let layout = ThinStr::layout_for(len);
            unsafe {
                let ptr: *mut ThinHeader = self.0.as_ptr();
                dealloc(ptr.cast::<u8>(), layout);
            }
        }
    }
}'''
